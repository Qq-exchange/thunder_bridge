    "amqp-connection-manager": "^2.0.0",
    "amqplib": "^0.5.2",
    "aws-sdk": "^2.480.0",
    "bignumber.js": "^7.2.1",
    "dotenv": "^5.0.1",
    "envalid": "^6.0.1",
    "http-list-provider": "0.0.5",
    "ioredis": "^3.2.2",
    "lodash": "^4.17.10",
    "node-fetch": "^2.1.2",
    "pino": "^6.2.1",
    "pino-pretty": "^2.0.1",
    "promise-limit": "^2.7.0",
    "promise-retry": "^1.1.1",
    "redlock": "^3.1.2",
    "typescript": "^3.8.3",
    "web3": "1.2.6",
    "web3-utils": "1.2.6"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/sinon": "^9.0.0",
    "bn-chai": "^1.0.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^3.6.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "mocha": "^5.2.0",
    "ndjson": "^1.5.0",
    "nyc": "^12.0.2",
    "openzeppelin-solidity": "^1.10.0",
    "prettier": "^1.12.1",
    "proxyquire": "^2.0.1",
    "sha3": "2.1.1",
    "simple-statistics": "^6.1.0",
    "sinon": "^6.1.0",
    "truffle": "^5.1.24",
    "truffle-contract": "^4.0.31",
    "truffle-flattener": "^1.2.3",
    "ts-node": "^8.9.1"
  },
  "engines": {
    "node": ">= 8.9"
  }
